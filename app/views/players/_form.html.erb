<%= form_for(@player, url: team_players_path) do |f| %>
<%= f.label :playername %>
<%= f.text_field :playername %>
<%= f.label :position %>
<%= f.text_field :position%>
<%= f.hidden_field :team_id %>
<%= f.submit %>
<% end %>

<% @player.errors.full_messages do |m| %>
    <div class ="errors">
    <%= m %>
    </div>
    <% end %>